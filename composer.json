{
    "name": "shakahl/pharover",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "description": "Command line tool to send push notifications through the Pushover service.",
    "keywords": [
        "php",
        "phar",
        "pushover",
        "pharover",
        "notification",
        "push"
    ],
    "__repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/shakahl/pharover.git"
        }
    ],
    "homepage": "https://github.com/shakahl/pharover",
    "authors": [
        {
            "name": "Soma",
            "email": "szelpalsoma@gmail.com",
            "homepage": "https://github.com/shakahl",
            "role": "Developer"
        }
    ],
    "bin": ["bin/pharover"],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/console": "^2.7",
        "symfony/process": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "App\\TestCases\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
